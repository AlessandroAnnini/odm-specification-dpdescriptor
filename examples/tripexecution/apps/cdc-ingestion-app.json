{
    "fullyQualifiedName": "urn:dpds:com.company-xyz:dataproducts:tripExecution:1:applications:cdcIngestion",
    "version": "1.0.1",
    "description": "The app that offloads the Trip aggregate from  TMS using CDC",
    "platform": "westeurope.azure:debezium",
    "applicationType":"stream-sourcing",
    "buildInfo": {
        "service": {
            "$ref":"http://jenkins"
        },
        "template": {
            "mediaType": "text/jenkinsfile",
            "$ref": "https://github.com/opendatamesh-initiative/cd-ingestion-app/blob/main/jenkinsfile"
        },
        "configurations": {
            "TARGET_STAGE": "BUILD"
        }
    },
    "deployInfo": {
        "service": {
            "mediaType": "application",
            "$ref": "https://jenkins.company-xyz.com/api/v1/planes/utility/integration-services/builds"
        },
        "template": {
            "specification": "custom-spec",
            "specificationVersion": "1.0.0",
            "contentMediaType": "application/json",
            "definition": {
              "pipeName": "cdcIngestion",
            }
        },
        "configurations": {
            "START_STAGE": "BUILD",
            "TARGET_STAGE": "DEPLOY"
        }
    }
}
